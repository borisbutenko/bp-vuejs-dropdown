    "babel-loader": "^7.1.2",
    "babel-preset-latest": "^6.24.1",
    "compression-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "envify": "^4.1.0",
    "happypack": "^4.0.0",
